{
  "name": "foka",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "HOST=0.0.0.0 PORT=${PORT:-3000} NEXT_DISABLE_LIGHTNINGCSS=1 LIGHTNINGCSS_DISABLE=1 next dev --turbopack",
    "build": "NEXT_DISABLE_LIGHTNINGCSS=1 LIGHTNINGCSS_DISABLE=1 next build",
    "start": "NEXT_DISABLE_LIGHTNINGCSS=1 LIGHTNINGCSS_DISABLE=1 next start -H 0.0.0.0 -p ${PORT:-3000}",
    "lint": "biome check",
    "format": "biome format --write"
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "optionalDependencies": {
    "@next/swc-linux-x64-gnu": "15.5.4",
    "@next/swc-linux-x64-musl": "15.5.4",
    "lightningcss": "^1.30.2"
  }
}
